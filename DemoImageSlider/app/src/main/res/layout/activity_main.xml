<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <pulkit.demo_real_parallex.com.demoimageslider.realParallex.RealHorizontalScrollView
        android:id="@+id/main_horizontal_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:src="@mipmap/background" />

    <pulkit.demo_real_parallex.com.demoimageslider.realParallex.RealViewPager
        android:id="@+id/main_view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:parallaxVelocity="0.2" />

</RelativeLayout>
